{
  "name": "Boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint --ext .js --ext .jsx src",
    "lint:fix": "eslint --ext .js --ext .jsx src --fix",
    "utest": "mocha --compilers js:babel-core/register -r ./test/helper.js --recursive ./src/**/*.spec.js",
    "utest:watch": "mocha --compilers js:babel-core/register -r ./test/helper.js --recursive --watch ./src/**/*.spec.js",
    "utest:current": "mocha --compilers js:babel-core/register -r ./test/helper.js --recursive --grep @current ./src/**/*.spec.js",
    "utest:current:watch": "mocha --compilers js:babel-core/register -r ./test/helper.js --recursive --watch --grep @current ./src/**/*.spec.js",
    "copyFiles": "node copyFiles",
    "build": "set NODE_ENV=production && webpack",
    "serve": "http-server -p 8080 ./release",
    "utest:teamcity": "mocha --compilers js:babel-core/register -r ./test/helper.js \"./src/**/*.spec.js\"",
    "build:teamcity": "export NODE_ENV=production && webpack"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Michael Huie",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-root-import": "^5.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.1",
    "chai-subset": "^1.5.0",
    "compression-webpack-plugin": "^1.1.2",
    "css-loader": "^0.28.5",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.5.0",
    "ncp": "^2.0.0",
    "nock": "^9.0.14",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "precss": "^2.0.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "fingerprintjs": "^0.5.3",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.5.3",
    "promise-polyfill": "^6.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
